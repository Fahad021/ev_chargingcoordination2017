title = {{Risk-Aware Day-Ahead Scheduling and Real-time Dispatch for Electric Vehicle Charging}},
volume = {5},
year = {2014}
}
@article{Saravanan2013,
abstract = {Unit commitment (UC) is an optimization problem used to determine the operation schedule of the generating units at every hour interval with varying loads under different constraints and environments. Many algorithms have been invented in the past five decades for optimization of the UC problem, but still researchers are working in this field to find new hybrid algorithms to make the problem more realistic. The importance of UC is increasing with the constantly varying demands. There- fore, there is an urgent need in the power sector to keep track of the latest methodologies to further optimize the working criterions of the generating units. This paper focuses on providing a clear review of the latest techniques employed in optimizing UC problems for both stochastic and deterministic loads, which has been acquired from many peer reviewed published papers. It has been divided into many sections which include various constraints based on profit, security, emission and time. It emphasizes not only on deregulated and regulated environments but also on renewable energy and distributed generating systems. In terms of contributions, the detailed analysis of all the UC algorithms has been discussed for the benefit of new researchers interested in working in this field},
author = {Saravanan, B. and Das, Siddharth and Sikri, Surbhi and Kothari, D. P.},
doi = {10.1007/s11708-013-0240-3},
file = {:C$\backslash$:/Users/Fabian Neumann/OneDrive/Studium/UOE/Dissertation/Literature/Sync/Saravanan et al/2013{\_}Saravanan et al.{\_}A solution to the unit commitment problem-a review.pdf:pdf},
issn = {20951701},
journal = {Frontiers in Energy},
keywords = {deterministic load,evolutionary programming (EP),hybrid,optimization,stochastic load,unit commitment (UC)},
number = {2},
pages = {223--236},
title = {{A solution to the unit commitment problem-a review}},
volume = {7},
year = {2013}
}
@article{Richardson2010a,
abstract = {The pattern of electricity use in an individual domestic dwelling is highly dependent upon the activities of the occupants and their associated use of electrical appliances. This paper presents a high-resolution model of domestic electricity use that is based upon a combination of patterns of active occupancy (i.e. when people are at home and awake), and daily activity profiles that characterise how people spend their time performing certain activities. One-min resolution synthetic electricity demand data is created through the simulation of appliance use; the model covers all major appliances commonly found in the domestic environment. In order to validate the model, electricity demand was recorded over the period of a year within 22 dwellings in the East Midlands, UK. A thorough quantitative comparison is made between the synthetic and measured data sets, showing them to have similar statistical characteristics. A freely downloadable example of the model is made available and may be configured to the particular requirements of users or incorporated into other models. ?? 2010 Elsevier B.V. All rights reserved.},
