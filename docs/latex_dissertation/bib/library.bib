isbn = {978-1-4244-6037-3},
keywords = {Automatic control,Communication system control,Energy consumption,Hybrid electric vehicles,Information technology,Intelligent vehicles,Power grids,Process control,Quadratic programming,Smart Grids,Smart grids,electric vehicle charging,energy consumption,energy control,home energy control box,hybrid electric vehicle,hybrid electric vehicles,iterative methods,iterative strategy,pluggable hybrid electric vehicle,power control,power grid,quadratic programming},
