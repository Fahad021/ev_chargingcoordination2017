isbn = {03062619},
issn = {03062619},
journal = {Applied Energy},
keywords = {Bi-level model,Demand response,Energy storage system,Uncertain,Virtual power plant},
month = {jun},
pages = {184--199},
publisher = {Elsevier Ltd},
title = {{A bi-level stochastic scheduling optimization model for a virtual power plant connected to a wind–photovoltaic–energy storage system considering the uncertainty and demand response}},
volume = {171},
