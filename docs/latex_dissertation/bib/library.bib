@article{Ghosh2013a,
abstract = {Renewable energy and Electric Vehicles (EV) show potential to be promising solutions for energy cost saving and emission reduction. However, the integration of renewable energy generation into the electric grid can be difficult, because of the source intermittency and inconsistency with energy usage. In this paper, we study the problem of allocating energy from renewable sources to EVs in an energy efficient manner. We assume that the renewable energy supply is time variant and possibly unpredictable. EVs' charging requests should be satisfied within a specified time frame, which may incur a cost of drawing extra energy (possibly non-renewable energy) if the renewable energy supply is not sufficent to meet the deadlines and may also reduce energy efficiency. We formulate a stochastic optimization problem based on queueing model to minimize the time average cost of using other energy sources (and hence make most usage of the renewable energy source). The proposed approach fully considers the individual charging rate limit and deadline of each EV. The Lyapunov optimization technique is used to solve the problem. The developed dynamic control algorithm does not require knowledge of the statistics of the time-varying renewable energy generation, EV charging demand process, or extra energy pricing.},
author = {Ghosh, Prasanta},
doi = {10.1109/PESMG.2013.6672568},
file = {:C$\backslash$:/Users/Fabian Neumann/OneDrive/Studium/UOE/Dissertation/Literature/Sync/Ghosh/2013{\_}Ghosh{\_}Energy efficient algorithms for Electric Vehicle charging with intermittent renewable energy sources.pdf:pdf},
isbn = {978-1-4799-1303-9},
issn = {1944-9925},
journal = {2013 IEEE Power {\&} Energy Society General Meeting},
keywords = {Batteries,Delays,EV charging demand process,Heuristic algorithms,Lyapunov optimization,Lyapunov optimization technique,Optimization,Real-time systems,Renewable energy sources,Scheduling,charging rate limit,charging requests,dynamic control algorithm,electric grid,electric vehicle,electric vehicle charging,electric vehicles,emission reduction,energy conservation,energy cost saving,energy efficiency,energy efficient algorithms,energy pricing,energy storage,energy usage,intermittent renewable energy sources,optimisation,queueing model,renewable energy,renewable energy sources,renewable energy supply,secondary cells,source inconsistency,source intermittency,stochastic optimization problem,stochastic processes,time average cost,time frame,time-varying renewable energy generation},
pages = {1--5},
title = {{Energy efficient algorithms for Electric Vehicle charging with intermittent renewable energy sources}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6672568},
year = {2013}
}
