isbn = {1949-3053},
issn = {1949-3053},
journal = {IEEE Transactions on Smart Grid},
keywords = {Distribution systems,load factor,load management,load variance,losses minimization,plug-in hybrid vehicle (PHEV),smart charging},
month = {mar},
