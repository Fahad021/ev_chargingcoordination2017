pages = {285--297},
publisher = {Elsevier Ltd},
title = {{Optimal scheduling of renewable micro-grids considering plug-in hybrid electric vehicle charging demand}},
url = {http://dx.doi.org/10.1016/j.energy.2016.01.063},
