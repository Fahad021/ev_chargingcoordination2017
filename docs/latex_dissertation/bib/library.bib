title = {{An Optimized EV Charging Model Considering TOU Price and SOC Curve}},
volume = {3},
year = {2012}
}
@unpublished{Ocker2016,
abstract = {This paper presents an economic analysis of the sec- ondary balancing power market in Germany. First, the operating principle of the market and the current market situation are illustrated. Then, a theoretic approach for the derivation of the optimal bidding strategy is presented that leads to optimal power and energy bids. These bids are computed for different market scenarios such as high-price and low-price periods. Finally, the implications of a high penetration of renewable energies on the German SR market are examined.},
address = {Karlsruhe, Aachen},
author = {Ocker, Fabian and Ehrhart, Karl-Martin and Ott, M.},
