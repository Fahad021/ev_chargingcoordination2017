@article{Saber2010,
abstract = {A gridable vehicle (GV) can be used as a small portable power plant (S3P) to enhance the security and reliability of utility grids. Vehicle-to-grid (V2G) technology has drawn great interest in the recent years and its success depends on intelligent scheduling of GVs or S3Ps in constrained parking lots. V2G can reduce dependencies on small expensive units in existing power systems, resulting in reduced operation cost and emissions. It can also increase reserve and reliability of existing power systems. Intelligent unit commitment (UC) with V2G for cost and emission optimization in power system is presented in this paper. As number of gridable vehicles in V2G is much higher than small units of existing systems, UC with V2G is more complex than basic UC for only thermal units. Particle swarm optimization (PSO) is proposed to balance between cost and emission reductions for UC with V2G. PSO can reliably and accurately solve this complex constrained optimization problem easily and quickly. In the proposed solution model, binary PSO optimizes on/off states of power generating units easily. Vehicles are presented by integer numbers instead of zeros and ones to reduce the dimension of the problem. Balanced hybrid PSO optimizes the number of gridable vehicles of V2G in the constrained parking lots. Balanced PSO provides a balance between local and global searching abilities, and finds a balance in reducing both operation cost and emission. Results show a considerable amount of cost and emission reduction with intelligent UC with V2G. Finally, the practicality of UC with V2G is discussed for real-world applications. {\textcopyright} 2009 Elsevier B.V. All rights reserved.},
author = {Saber, Ahmed Yousuf and Venayagamoorthy, Ganesh Kumar},
doi = {10.1016/j.jpowsour.2009.08.035},
file = {:C$\backslash$:/Users/Fabian Neumann/OneDrive/Studium/UOE/Dissertation/Literature/Sync/Saber, Venayagamoorthy/2010{\_}Saber, Venayagamoorthy{\_}Intelligent unit commitment with vehicle-to-grid -A cost-emission optimization.pdf:pdf},
isbn = {0378-7753},
issn = {03787753},
journal = {Journal of Power Sources},
keywords = {Cost,Emission,Gridable vehicles,Particle swarm optimization,UC,V2G},
number = {3},
pages = {898--911},
title = {{Intelligent unit commitment with vehicle-to-grid -A cost-emission optimization}},
volume = {195},
year = {2010}
}
@article{Acha2010b,
abstract = {The deployment of plug-in hybrid vehicles (PHEVs) and micro-combined heat and power (micro-CHP) technologies create the opportunity to enhance energy service networks by coordinating these small-scale devices under various dispatch schemes. Furthermore, if vehicle-to-grid (V2G) and thermal storage features are considered, these embedded technologies could have even a greater impact on network performance. Therefore, this work presents a holistic modelling framework which serves as a tool to optimise in a coordinated manner the operation of natural gas and electrical infrastructures under the presence of distributed energy resources (DERs). The modelling tool developed, under the name TCOPF, is a decision maker entity that aggregates and coordinates the available DERs according to utility conditions. As a consequence, the tool functions by employing an integrated time coordinated electric and natural gas optimal power ow. Hence, once the results are analysed, the output allows stakeholders to identify techno-economical tradeo s that might be encountered by applying particular operating strategies and load control approaches. To illustrate the value of the modelling tool, a case study is assessed by evaluating di erent objective functions which simulate diverse operating strategies of utilities. The examples prove the TCOPF acts as an ecient intermediary entity that manages cost-e ective interactions between the connected technologies and the distribution network operators (DNOs). Results show considerable bene ts in operating electrical networks while simultaneously having mild e ects in natural gas networks. Furthermore, the modelling tool describes the optimal dispatch patterns distributed energy resources should follow in order to improve the performance of urban energy service networks. Thus, the TCOPF program o ers a fresh perspective for stakeholders wishing to successfully integrate and coordinate in a seamlessly fashion the operation of distributed energy resources with the operating strategies of energy utilities.},
author = {Acha, Salvador},
file = {:C$\backslash$:/Users/Fabian Neumann/OneDrive/Studium/UOE/Dissertation/Literature/Sync/Acha/2010{\_}Acha{\_}Impacts of Embedded Technologies on Optimal Operation of Energy Service Networks.pdf:pdf},
journal = {Network},
pages = {200},
title = {{Impacts of Embedded Technologies on Optimal Operation of Energy Service Networks}},
year = {2010}
}
@article{Flath2012,
abstract = {The introduction of smart meter technology is a great challenge for the$\backslash$nGerman energy industry. It requires not only large investments in the$\backslash$ncommunication and metering infrastructure, but also a redesign of$\backslash$ntraditional business processes. The newly incurring costs cannot be$\backslash$nfully passed on to the end customers. One option to counterbalance these$\backslash$nexpenses is to exploit the newly generated smart metering data for the$\backslash$ncreation of new services and improved processes. For instance,$\backslash$nperforming a cluster analysis of smart metering data focused on the$\backslash$ncustomers' time-based consumption behavior allows for a detailed$\backslash$ncustomer segmentation. In the article we present a cluster analysis$\backslash$nperformed on real-world consumption data from a smart meter project$\backslash$nconducted by a German regional utilities company. We show how to$\backslash$nintegrate a cluster analysis approach into a business intelligence$\backslash$nenvironment and evaluate this artifact as defined by design science. We$\backslash$ndiscuss the results of the cluster analysis and highlight options to$\backslash$napply them to segment-specific tariff design.},
