doi = {10.1007/s12599-011-0201-5},
file = {:C$\backslash$:/Users/Fabian Neumann/OneDrive/Studium/UOE/Dissertation/Literature/Sync/Flath et al/2012{\_}Flath et al.{\_}Cluster analysis of smart metering data An implementation in practice.pdf:pdf},
